.stars-container {
  display: flex;
  flex-direction: row;
  text-align: center;
}

.stars {
  display: flex;
  flex-direction: row;
  height: 40rpx;
  margin-right: 24rpx;
  margin-top: 2rpx;
}

.stars image {
  padding-left: 3rpx;
  height: 16rpx;
  width: 16rpx;
}

.star-score{
  font-size: 24rpx;
   color: #1f3463;
}


.star_bg {
  margin: 0rpx 10rpx;
  width: 100rpx;
  height: 20rpx;
  background-image: url('data:image/png;base64,');
  background-size: contain;
}

.star_over {
  height: 20rpx;
  background-image: url('data:image/png;base64,');
  background-size: contain;
}